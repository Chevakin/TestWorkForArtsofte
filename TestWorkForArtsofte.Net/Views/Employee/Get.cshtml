@model IEnumerable<EmployeeDisplayViewModel>

<table class="table">
    <tr>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Возраст</th>
        <th>Пол</th>
        <th>Отдел</th>
        <th>Язык программирования</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Name</td>
            <td>@employee.Surname</td>
            <td>@employee.Age</td>
            <td>@employee.Gender</td>
            <td>@employee.Department</td>
            <td>@employee.ProgrammingLanguage</td>
            @*<td><a asp-action="Details" asp-route-id="@customer.ID">Подробнее</a></td>*@
        </tr>
    }
</table>